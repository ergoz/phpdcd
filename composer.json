{
    "name": "ergoz/phpdcd",
    "description": "Not Official version of PHPDCD Dead Code Detector (DCD) for PHP code handled with Composer.",

    "require": {
        "php": ">=5.3.0",
        "pdepend/pdepend": ">=1.1.1",
        "symfony/symfony": "*"
    },

    "include-path": [
        "../../ergoz/phpdcd/PHPDCD"
    ],
    "bin": ["PHPDCD/Detector.php"]
    
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.phpunit.de"
        }
    ],
    "require": {
        "pear.phpunit.de/Finder": "*",
        "pear.phpunit.de/PHP_Timer": "*",
        "pear.phpunit.de/PHP_TokenStream": "*",
        "components.ez.no/ConsoleTools": "*",
        "pear.phpunit.de/PHP_Timer": "*",
        "pear.phpunit.de/PHP_Timer": "*",
    }
}
}
